[versionner]
file = ./VERSION
date_format = %Y-%m-%d
up_part = patch
default_init_version = 0.1.0

[vcs]
engine = git
commit_message = '%(version)s'

[file:prodperfect/__init__.py]
enabled = true
search = ^\s*__version__\s*=.*$
replace = __version__ = '%(version)s'
date_format = %Y-%m-%d
match = line
search_flags =
encoding = utf-8
